<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
		<b:Hero>
			<b:Row ui:field="dupError"/>
			<b:Heading size="2">Add A Role</b:Heading>
			<b:ResponsiveNavbar>
				<b:Nav>
					<b:NavText>Priority</b:NavText>
				</b:Nav>
				<b:Nav>			
					<b:NavForm ui:field="priority" size="1"></b:NavForm>
				</b:Nav>
				<b:Nav>
					<b:NavText>Role</b:NavText>
				</b:Nav>
				<b:Nav>
					<b:NavForm ui:field="roleName"/>
				</b:Nav>
				<b:Nav>
					<b:Button type="WARNING" ui:field="addRoleButton">Add Role</b:Button>
				</b:Nav>
			</b:ResponsiveNavbar>
			<g:FlexTable ui:field="roleTable"/>
		</b:Hero>
	</g:HTMLPanel>
</ui:UiBinder> 